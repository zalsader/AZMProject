	<queryString language="SQL">
		<![CDATA[SELECT
     BUY_ORDER."ORDER_ID" AS BUY_ORDER_ORDER_ID,
     BUY_ORDER."ORDER_DATE" AS BUY_ORDER_ORDER_DATE,
     BUY_ORDER."SUPPLIER_ID" AS BUY_ORDER_SUPPLIER_ID,
     DETAILED_BUY_ORDER."ORDER_ID" AS DETAILED_BUY_ORDER_ORDER_ID,
     DETAILED_BUY_ORDER."ITEM_ID" AS DETAILED_BUY_ORDER_ITEM_ID,
     DETAILED_BUY_ORDER."BUY_QUANTITY" AS DETAILED_BUY_ORDER_BUY_QUANTIT,
     DETAILED_BUY_ORDER."UNIT_PRICE" AS DETAILED_BUY_ORDER_UNIT_PRICE,
     ITEMS."ITEM_ID" AS ITEMS_ITEM_ID,
     ITEMS."ITEM_NAME" AS ITEMS_ITEM_NAME,
     ITEMS."ITEM_UNIT" AS ITEMS_ITEM_UNIT,
     ITEMS."AVAILABLE_QUANTITY" AS ITEMS_AVAILABLE_QUANTITY,
     ITEMS."MINIMUM_QUANTITY" AS ITEMS_MINIMUM_QUANTITY,
     SUPPLIERS."SUPPLIER_ID" AS SUPPLIERS_SUPPLIER_ID,
     SUPPLIERS."SUPPLIER_NAME" AS SUPPLIERS_SUPPLIER_NAME,
     SUPPLIERS."SUPPLIER_ADDRESS" AS SUPPLIERS_SUPPLIER_ADDRESS,
     SUPPLIERS."PHONE_NO" AS SUPPLIERS_PHONE_NO,
     SUPPLIERS."FAX_NO" AS SUPPLIERS_FAX_NO
FROM
     "AZM"."BUY_ORDER" BUY_ORDER INNER JOIN "AZM"."DETAILED_BUY_ORDER" DETAILED_BUY_ORDER ON BUY_ORDER."ORDER_ID" = DETAILED_BUY_ORDER."ORDER_ID"
     INNER JOIN "AZM"."ITEMS" ITEMS ON DETAILED_BUY_ORDER."ITEM_ID" = ITEMS."ITEM_ID"
     INNER JOIN "AZM"."SUPPLIERS" SUPPLIERS ON BUY_ORDER."SUPPLIER_ID" = SUPPLIERS."SUPPLIER_ID"]]>
	</queryString>
