	<queryString language="SQL">
		<![CDATA[SELECT
     ITEMS."ITEM_ID" AS ITEMS_ITEM_ID,
     ITEMS."ITEM_NAME" AS ITEMS_ITEM_NAME,
     ITEMS."ITEM_UNIT" AS ITEMS_ITEM_UNIT,
     ITEMS."AVAILABLE_QUANTITY" AS ITEMS_AVAILABLE_QUANTITY,
     ITEMS."MINIMUM_QUANTITY" AS ITEMS_MINIMUM_QUANTITY,
     REQUEST_OF_ITEMS."REQUEST_ID" AS REQUEST_OF_ITEMS_REQUEST_ID,
     REQUEST_OF_ITEMS."REQUEST_DATE" AS REQUEST_OF_ITEMS_REQUEST_DATE,
     REQUEST_OF_ITEMS."DEPT_ID" AS REQUEST_OF_ITEMS_DEPT_ID,
     REQUEST_OF_ITEMS."ACCEPTED" AS REQUEST_OF_ITEMS_ACCEPTED,
     DETAILED_REQUEST_OF_ITEMS."REQUEST_ID" AS DETAILED_REQUEST_OF_ITEMS_REQU,
     DETAILED_REQUEST_OF_ITEMS."ITEM_ID" AS DETAILED_REQUEST_OF_ITEMS_ITEM,
     DETAILED_REQUEST_OF_ITEMS."REQUESTED_QUANTITY" AS DETAILED_REQUEST_OF_ITEMS_REQU,
     DEPARTMENTS."DEPT_ID" AS DEPARTMENTS_DEPT_ID,
     DEPARTMENTS."DEPT_NAME" AS DEPARTMENTS_DEPT_NAME,
     DEPARTMENTS."DEPT_MNGR_ID" AS DEPARTMENTS_DEPT_MNGR_ID
FROM
     "AZM"."ITEMS" ITEMS INNER JOIN "AZM"."DETAILED_REQUEST_OF_ITEMS" DETAILED_REQUEST_OF_ITEMS ON ITEMS."ITEM_ID" = DETAILED_REQUEST_OF_ITEMS."ITEM_ID"
     INNER JOIN "AZM"."REQUEST_OF_ITEMS" REQUEST_OF_ITEMS ON DETAILED_REQUEST_OF_ITEMS."REQUEST_ID" = REQUEST_OF_ITEMS."REQUEST_ID"
     INNER JOIN "AZM"."DEPARTMENTS" DEPARTMENTS ON REQUEST_OF_ITEMS."DEPT_ID" = DEPARTMENTS."DEPT_ID"]]>
	</queryString>
