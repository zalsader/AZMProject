	<queryString language="SQL">
		<![CDATA[SELECT
     DEPARTMENTS."DEPT_ID" AS DEPARTMENTS_DEPT_ID,
     DEPARTMENTS."DEPT_NAME" AS DEPARTMENTS_DEPT_NAME,
     DEPARTMENTS."DEPT_MNGR_ID" AS DEPARTMENTS_DEPT_MNGR_ID,
     DELIVERY_FORM."DELIVERY_ID" AS DELIVERY_FORM_DELIVERY_ID,
     DELIVERY_FORM."DELIVERY_DATE" AS DELIVERY_FORM_DELIVERY_DATE,
     DELIVERY_FORM."DEPT_ID" AS DELIVERY_FORM_DEPT_ID,
     DELIVERY_FORM."CONFIRMED" AS DELIVERY_FORM_CONFIRMED,
     DETAILED_DELIVERY."DELIVERY_ID" AS DETAILED_DELIVERY_DELIVERY_ID,
     DETAILED_DELIVERY."ITEM_ID" AS DETAILED_DELIVERY_ITEM_ID,
     DETAILED_DELIVERY."DELIVERED_QUANTITY" AS DETAILED_DELIVERY_DELIVERED_QU,
     ITEMS."ITEM_ID" AS ITEMS_ITEM_ID,
     ITEMS."ITEM_NAME" AS ITEMS_ITEM_NAME,
     ITEMS."ITEM_UNIT" AS ITEMS_ITEM_UNIT,
     ITEMS."AVAILABLE_QUANTITY" AS ITEMS_AVAILABLE_QUANTITY,
     ITEMS."MINIMUM_QUANTITY" AS ITEMS_MINIMUM_QUANTITY
FROM
     "AZM"."DEPARTMENTS" DEPARTMENTS INNER JOIN "AZM"."DELIVERY_FORM" DELIVERY_FORM ON DEPARTMENTS."DEPT_ID" = DELIVERY_FORM."DEPT_ID"
     INNER JOIN "AZM"."DETAILED_DELIVERY" DETAILED_DELIVERY ON DELIVERY_FORM."DELIVERY_ID" = DETAILED_DELIVERY."DELIVERY_ID"
     INNER JOIN "AZM"."ITEMS" ITEMS ON DETAILED_DELIVERY."ITEM_ID" = ITEMS."ITEM_ID"]]>
